set autocorrect=none
set correct=none
set autolist=ambiguous
alias nc netcat
alias l "ls -AlhF --color=auto"
alias ls "ls -F --color=auto"
alias mp "ps aux | grep -w $USER"
alias cwdcmd ""
set prompt='%B%n@%m%b %c2%#'
setenv LYNX_CFG ~/.lynxrc
umask 002
setenv CVSROOT /var/lib/cvs
setenv EDITOR /usr/bin/vim
alias svndiff 'svn diff --diff-cmd=`which diff` -x -u -x -p \!*'
setenv PATH ~/bin:${PATH}
